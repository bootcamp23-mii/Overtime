	<queryString>
		<![CDATA[SELECT
 ( SELECT COUNT(OVERTIME."STATUS")      
FROM
     "ADMIN"."TIME_SHEET" TIME_SHEET INNER JOIN "ADMIN"."OVERTIME" OVERTIME ON TIME_SHEET."ID" = OVERTIME."TIME_SHEET" WHERE OVERTIME.STATUS = '1' AND TIME_SHEET.TIME_SHEET_DATE BETWEEN TO_DATE ('20190301','YYYYMMDD') AND TO_DATE('20190401','YYYYMMDD') ) AS APPROVED,
( SELECT COUNT(OVERTIME."STATUS")    
FROM
     "ADMIN"."TIME_SHEET" TIME_SHEET INNER JOIN "ADMIN"."OVERTIME" OVERTIME ON TIME_SHEET."ID" = OVERTIME."TIME_SHEET" WHERE OVERTIME.STATUS = '-1' AND TIME_SHEET.TIME_SHEET_DATE BETWEEN TO_DATE ('20190301','YYYYMMDD') AND TO_DATE('20190401','YYYYMMDD')) AS REJECTED,
( SELECT COUNT(OVERTIME."STATUS")    
FROM
     "ADMIN"."TIME_SHEET" TIME_SHEET INNER JOIN "ADMIN"."OVERTIME" OVERTIME ON TIME_SHEET."ID" = OVERTIME."TIME_SHEET" WHERE OVERTIME.STATUS = '0' AND TIME_SHEET.TIME_SHEET_DATE BETWEEN TO_DATE ('20190301','YYYYMMDD') AND TO_DATE('20190401','YYYYMMDD')) AS NOT_CONFIRMED,     
EMPLOYEE."ID" AS EMPLOYEE_ID,
     EMPLOYEE."NAME" AS EMPLOYEE_NAME,
     OVERTIME."OVERTIME_DATE" AS OVERTIME_OVERTIME_DATE,
     OVERTIME."STATUS" AS OVERTIME_STATUS,
     OVERTIME."TIME_DURATION" AS OVERTIME_TIME_DURATION
FROM
     "ADMIN"."EMPLOYEE" EMPLOYEE INNER JOIN "ADMIN"."TIME_SHEET" TIME_SHEET ON EMPLOYEE."ID" = TIME_SHEET."EMPLOYEE"
     INNER JOIN "ADMIN"."OVERTIME" OVERTIME ON TIME_SHEET."ID" = OVERTIME."TIME_SHEET"
WHERE TIME_SHEET.TIME_SHEET_DATE BETWEEN TO_DATE ('20190301','YYYYMMDD') AND TO_DATE('20190401','YYYYMMDD')
]]>
	</queryString>
